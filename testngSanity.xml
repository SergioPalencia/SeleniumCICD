<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">
  
  <groups>
  	<run>
  		<include name = "Sanity"/>
  	</run>
  </groups>
  
  <listeners>
  	<listener class-name="SPB.utilidadesPruebas.Listeners"/>
  </listeners>
  
  <test thread-count="5" name="TC Clientes">
    <classes>
      <class name="SPB.casosPrueba.TC_Clientes"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="TC Errores">
    <classes>
      <class name="SPB.casosPrueba.TC_Errores"/>
    </classes>
  </test> <!-- Test -->

</suite> <!-- Suite -->
